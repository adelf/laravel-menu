{
    "name": "adelf/laravel-menu",
    "description": "Menu component for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adel Fayzrakhmanov",
            "email": "adel.faiz@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Adelf\\LaravelMenu\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Adelf\\LaravelMenu\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adelf\\LaravelMenu\\LaravelMenuServiceProvider"
            ]
        }
    }
}
